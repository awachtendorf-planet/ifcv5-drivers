  </complexType>
  </element>

  <element name="PostList">
  <complexType>
    <sequence>
      <element name="PostListItem" type="PostListSchema_O:PostListItem" maxOccurs="unbounded" />
    </sequence>
    <attribute name="SequenceNumber" type="integer" use="required" />
