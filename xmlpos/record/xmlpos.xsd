  <complexType>
    <attribute name="Date" type="date" use="required" />
    <attribute name="Time" type="time" use="required" />
    <attribute name="VerNum" type="string" use="required" />
  </complexType>
  </element>

  <element name="LinkStart">
  <complexType>
